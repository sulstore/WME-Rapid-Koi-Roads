{
  "manifest_version": 3,
  "name": "WME RAPID KOI ROADS",
  "version": "0.8.2.1",
  "description": "Agiliza a sugestão de mapeamento e criação de ruas",
  "icons": {
    "48": "logo.png",
    "128": "logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "rapidkoiroads.js",
        "notaskoi.html"
      ],
      "matches": [
        "https://www.waze.com/*",
        "https://beta.waze.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.waze.com/*/editor*",
        "https://www.waze.com/editor*",
        "https://beta.waze.com/*/editor*",
        "https://beta.waze.com/editor*"
      ],
      "exclude_matches": [
        "https://www.waze.com/*/user/editor/*",
        "https://www.waze.com/discuss/*"
      ],
      "js": ["boot.js"],
      "run_at": "document_idle"
    }
  ]
}












